{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "The exchange rate (from-to-rate)",
  "type": "object",
  "additionalProperties" : false,
  "generate-builders": true,
  "id": "ExchangeRate",
  "properties": {
    "from":{
      "type":"string",
      "required": true
    },
    "to":{
      "type":"string",
      "required": true
    },
    "rate": {
      "javaType": "Double",
      "required": true
    }
  }
}